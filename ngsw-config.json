{
    "$schema": "./node_modules/@angular/service-worker/config/schema.json",
    "index": "/index.html",
    "assetGroups": [
        {
            "name": "cdn-assets",
            "installMode": "lazy",
            "updateMode": "prefetch",
            "resources": {
                "files": [
                    "/assets/primeicons/raw-svg/*.(svg|jpg|jpeg|png|webp|avif|icon)",
                    "/assets/primeicons/fonts/*.(svg|eot|ttf|woff|woff2)"
                ]
            }
        },
        {
            "name": "app",
            "installMode": "prefetch",
            "resources": {
                "files": [
                    "/favicon.ico",
                    "/index.csr.html",
                    "/index.html",
                    "/manifest.webmanifest",
                    "/*.css",
                    "/*.scss",
                    "/*.js"
                ]
            }
        },
        {
            "name": "assets",
            "installMode": "prefetch",
            "updateMode": "prefetch",
            "resources": {
                "files": [
                    "/assets/images/*.(jpg|jpeg|png|webp|avif|icon)",
                    "/assets/sounds/*.(mp3|wav)"
                ]
            }
        },
        {
            "name": "leaflet-cdn",
            "installMode": "prefetch",
            "resources": {
                "urls": [
                    "https://unpkg.com/leaflet@1.9.4/dist/leaflet.css",
                    "https://unpkg.com/leaflet@1.9.4/dist/leaflet.js"
                ]
            }
        },
        {
            "name": "openstreetmap",
            "updateMode": "prefetch",
            "resources": {
                "urls": [
                    "https://tile.openstreetmap.org/*"
                ]
            }
        },
        {
            "name": "fonts",
            "installMode": "prefetch",
            "resources": {
                "files": [
                    "node_modules/primeicons/primeicons.css",
                    "node_modules/primeicons/fonts/*.woff2",
                    "node_modules/primeicons/fonts/*.woff",
                    "node_modules/primeicons/fonts/*.ttf"
                ]
            }
        }
    ],
    "dataGroups": [
        {
            "name": "api-calls",
            "urls": [
                "https://cumkqrjwsbyotaojeyxv.supabase.co/*"
            ],
            "cacheConfig": {
                "strategy": "freshness",
                "maxSize": 10000,
                "maxAge": "6h",
                "timeout": "5s"
            }
        },
        {
            "name": "assets",
            "urls": [
                "/assets/**"
            ],
            "cacheConfig": {
                "strategy": "performance",
                "maxSize": 10000,
                "maxAge": "6h",
                "timeout": "5s"
            }
        }
    ]
}