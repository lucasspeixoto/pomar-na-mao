{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/favicon.ico",
          "/index.csr.html",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.scss",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/assets/images/**", "/assets/sounds/*.(mp3|wav)"]
      }
    },
    {
      "name": "leaflet-cdn",
      "installMode": "prefetch",
      "resources": {
        "urls": [
          "https://unpkg.com/leaflet@1.9.4/dist/leaflet.css",
          "https://unpkg.com/leaflet@1.9.4/dist/leaflet.js"
        ]
      }
    },
    {
      "name": "openstreetmap",
      "installMode": "prefetch",
      "resources": {
        "urls": ["https://tile.openstreetmap.org/*"]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-calls",
      "urls": ["https://qmhdkjspsnhkwvgodlru.supabase.co/*"],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 100,
        "maxAge": "1d",
        "timeout": "5s"
      }
    },
    {
      "name": "assets",
      "urls": ["/assets/**"],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 200,
        "maxAge": "30d"
      }
    }
  ]
}
