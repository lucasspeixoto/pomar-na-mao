{
    "name": "pomar-na-mao",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "start:pwa": "npx http-server -p 8080 -c-1 dist/pomar-na-mao/browser",
        "watch": "ng build --watch --configuration development",
        "lint": "ng lint",
        "lint:fix": "ng lint --fix",
        "prettier": "npx prettier --write .",
        "format": "prettier --write \"**/*.{js,mjs,ts,mts,d.ts,html}\" --cache",
        "test": "ng test --browsers=ChromeHeadless",
        "test:watch": "ng test --watch",
        "test:headless": "ng test --browsers=ChromeHeadless --watch=false"
    },
    "private": true,
    "dependencies": {
