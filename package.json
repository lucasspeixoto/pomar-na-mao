{
  "name": "pomar-na-mao",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "start:pwa": "npx http-server -p 8080 -c-1 dist/pomar-na-mao/browser",
    "watch": "ng build --watch --configuration development",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "prettier": "npx prettier --write .",
    "test": "jest --no-cache",
    "test:watch": "jest --no-cache --watch",
    "test-isolated": "jest -c=jest-isolated.config.ts --no-cache",
    "test-esm": "node --experimental-vm-modules --no-warnings node_modules/jest/bin/jest.js -c=jest-esm.config.ts --no-cache",
    "test-esm-isolated": "node --experimental-vm-modules --no-warnings node_modules/jest/bin/jest.js -c=jest-esm-isolated.config.ts --no-cache",
    "cypress:web": "cypress open",
    "cypress:headless": "cypress run --browser electron",
    "prepare": "husky"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.1.0",
    "@angular/common": "^19.1.0",
    "@angular/compiler": "^19.1.0",
    "@angular/core": "^19.1.0",
    "@angular/forms": "^19.1.0",
    "@angular/platform-browser": "^19.1.0",
    "@angular/platform-browser-dynamic": "^19.1.0",
    "@angular/router": "^19.1.0",
    "@angular/service-worker": "^19.1.0",
    "@badeball/cypress-cucumber-preprocessor": "^22.0.1",
    "@supabase/supabase-js": "^2.48.1",
    "crypto-js": "^4.2.0",
    "leaflet": "^1.9.4",
    "ng-zorro-antd": "^19.0.2",
    "punycode": "^2.3.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "uuid": "^11.1.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.1.6",
    "@angular/cli": "^19.1.6",
    "@angular/compiler-cli": "^19.1.0",
    "@cypress/browserify-preprocessor": "^3.0.2",
    "@cypress/schematic": "^3.0.0",
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^29.5.14",
    "@types/leaflet": "^1.9.16",
    "@types/mocha": "^10.0.10",
    "angular-eslint": "19.1.0",
    "cypress": "^14.2.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.5.1",
    "lint-staged": "^15.4.3",
    "prettier": "^3.5.1",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.0"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": false,
    "stepDefinitions": "cypress/e2e/step-definitions",
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  }
}
